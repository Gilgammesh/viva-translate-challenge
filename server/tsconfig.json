{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"rootDir": "./src",
		"typeRoots": ["./src/types"],
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"skipLibCheck": true
	}
}
